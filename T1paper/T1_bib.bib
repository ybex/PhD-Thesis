isbn = {1748-3395},
issn = {17483395},
journal = {Nature Nanotechnology},
number = {6},
pages = {430--435},
pmid = {24727686},
publisher = {Nature Publishing Group},
title = {{Spin blockade and exchange in Coulomb-confined silicon double quantum dots}},
url = {http://dx.doi.org/10.1038/nnano.2014.63},
volume = {9},
year = {2014}
}
