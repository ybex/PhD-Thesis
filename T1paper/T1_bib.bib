eprint = {1410.2338},
file = {:Q$\backslash$:/spin-QED/Steffi Stuff/References/SiQubits/Morello/Muhonen2015{\_}RB.pdf:pdf},
issn = {1361648X},
journal = {Journal of Physics Condensed Matter},
keywords = {quantum information,silicon,single dopant},
number = {15},
