pages = {48--50},
pmid = {12511949},
title = {{Implementation of the Deutsch-Jozsa algorithm on an ion-trap quantum computer.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/12511949},
volume = {421},
year = {2003}
}
