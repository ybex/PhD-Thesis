pages = {1--4},
title = {{Qubit metrology for building a fault-tolerant quantum computer}},
volume = {1},
year = {2015}
}
@article{Greenbaum2015,
abstract = {Quantum gate set tomography (GST) has emerged as a promising method for the full characterization of quantum logic gates. In contrast to quantum process tomography (QPT), GST self-consistently and correctly accounts for state preparation and measurement (SPAM) errors. It therefore provides significantly more accurate estimates than QPT as gate fidelities increase into the fault-tolerant regime. We give a detailed review of GST and provide a self-contained guide to its implementation. The method is presented in a step-by-step fashion and relevant mathematical background material is included. Our goal is to demonstrate the utility of GST as both an accurate characterization technique and a simple and effective diagnostic tool. As an illustration, we compare the output of GST and QPT using simulated example data for a single qubit. In agreement with the original literature, we find that coherent errors are poorly estimated by QPT near quantum error correction thresholds, while GST is accurate in this regime.},
archivePrefix = {arXiv},
arxivId = {1509.02921},
author = {Greenbaum, Daniel},
eprint = {1509.02921},
file = {:Q$\backslash$:/spin-QED/Steffi Stuff/References/QauntumComputingTheory/Introduction to GST 1509.02921v1.pdf:pdf},
title = {{Introduction to Quantum Gate Set Tomography}},
url = {http://arxiv.org/abs/1509.02921},
year = {2015}
}
@article{Gard2014,
abstract = {Boson-sampling is a simplified model for quantum computing that may hold the key to implementing the first ever post-classical quantum computer. Boson-sampling is a non-universal quantum computer that is significantly more straightforward to build than any universal quantum computer proposed so far. We begin this chapter by motivating boson-sampling and discussing the history of linear optics quantum computing. We then summarize the boson-sampling formalism, discuss what a sampling problem is, explain why boson-sampling is easier than linear optics quantum computing, and discuss the Extended Church-Turing thesis. Next, sampling with other classes of quantum optical states is analyzed. Finally, we discuss the feasibility of building a boson-sampling device using existing technology.},
archivePrefix = {arXiv},
arxivId = {1406.6767},
author = {Gard, Bryan T. and Motes, Keith R. and Olson, Jonathan P. and Rohde, Peter P. and Dowling, Jonathan P.},
