author = {Goetz, Jan and Deppe, Frank and Haeberlein, Max and Wulschner, Friedrich and Zollitsch, Christoph W. and Meier, Sebastian and Fischer, Michael and Eder, Peter and Xie, Edwar and Fedorov, Kirill G. and Menzel, Edwin P. and Marx, Achim and Gross, Rudolf},
doi = {10.1063/1.4939299},
