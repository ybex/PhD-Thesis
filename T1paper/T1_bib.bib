number = {1},
pages = {1--7},
publisher = {Springer US},
title = {{Higher-order spin and charge dynamics in a quantum dot-lead hybrid system}},
url = {http://dx.doi.org/10.1038/s41598-017-12217-6},
volume = {7},
year = {2017}
}



