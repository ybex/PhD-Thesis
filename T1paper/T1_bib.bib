journal = {Reports on Progress in Physics},
number = {5},
pages = {1325--1382},
pmid = {20366408},
title = {{Cavity quantum electrodynamics}},
url = {http://stacks.iop.org/0034-4885/69/i=5/a=R02?key=crossref.2e5c0e61d04e1066e39543161ec7a7c4},
volume = {69},
year = {2006}
