isbn = {0031-9007},
issn = {10797114},
journal = {Physical Review Letters},
mendeley-groups = {Thesis},
number = {23},
pages = {1--5},
title = {{Single-shot readout and relaxation of singlet and triplet states in exchange-coupled P 31 electron spins in silicon}},
volume = {112},
year = {2014}
