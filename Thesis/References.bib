isbn = {1745-2473},
issn = {17452473},
journal = {Nature Physics},
mendeley-groups = {Thesis},
number = {12},
pages = {928--931},
title = {{Amplification and squeezing of quantum noise with a tunable Josephson metamaterial}},
volume = {4},
year = {2008}
}
