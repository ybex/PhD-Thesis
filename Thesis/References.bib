arxivId = {1509.08201},
author = {Curry, M. J. and England, T. D. and Bishop, N. C. and Ten-Eyck, G. and Wendt, J. R. and Pluym, T. and Lilly, M. P. and Carr, S. M. and Carroll, M. S.},
doi = {10.1063/1.4921308},
eprint = {1509.08201},
